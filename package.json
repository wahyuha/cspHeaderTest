{
  "name": "@linkaja/wco",
  "description": "Webview for Direct Debit",
  "private": true,
  "version": "0.0.1",
  "scripts": {
    "dev": "cp .env.dev .env && yarn replace-env && sapper dev -p 5000",
    "build": "sapper build -p 5010 --legacy && cp package.json __sapper__/build && cp -r static __sapper__/build",
    "start": "node __sapper__/build",
    "replace-env": "bash bash/replace-env.sh .env",
    "build-development": "cp .env.development.j2 .env && yarn replace-env && yarn build",
    "build-staging": "cp .env.staging.j2 .env && yarn replace-env && yarn build",
    "build-production": "cp .env.production.j2 .env && yarn replace-env && yarn build",
    "lint": "npx eslint --fix \"{,src/**/}*.{js,svelte,json}\""
  },
  "dependencies": {
    "axios": "^0.21.0",
    "body-parser": "^1.19.0",
    "compression": "^1.7.1",
    "cookie-parser": "^1.4.5",
    "crypto-es": "1.2.5",
    "crypto-js": "^3.3.0",
    "express": "^4.17.1",
    "express-healthcheck": "^0.1.0",
    "express-mung": "^0.5.1",
    "express-session": "^1.17.1",
    "helmet": "^4.3.1",
    "lodash": "^4.17.20",
    "node-rsa": "^1.1.1",
    "sapper-page-loading-bar": "^1.0.0",
    "session-file-store": "^1.5.0",
    "sirv": "^1.0.0",
    "ssh-key-decrypt": "^0.1.2",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/runtime": "^7.0.0",
    "@rollup/plugin-alias": "^3.1.1",
    "@rollup/plugin-babel": "^5.0.0",
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^8.0.0",
    "@rollup/plugin-replace": "^2.2.0",
    "@rollup/plugin-url": "^5.0.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "dotenv": "^8.2.0",
    "eslint": "^7.17.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-cypress": "^2.11.2",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-json": "^2.1.2",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-svelte3": "^3.0.0",
    "rollup": "^2.3.4",
    "rollup-plugin-svelte": "^6.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "sapper": "^0.28.0",
    "svelte": "^3.17.3"
  }
}
